@{
  Layout = "_Layout";
}

<body>
    <div class="container">
        <h1>List of Stylists</h1>
        @if (@Model.Count == 0)
        {
          <h3>No Stylists have been added yet!</h3>
        }
        @foreach (var stylist in Model)
        {
          <h3>@stylist.GetId()<a href='/stylists/@stylist.GetId()'>@stylist.GetName()</a></h3>
        }
        <p><a href='/'>Back Home</a></p>
    </div>
</body>
